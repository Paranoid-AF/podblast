{
  "name": "podblast",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "better-sqlite3": "^7.1.2",
    "cheerio": "^1.0.0-rc.5",
    "electron-is-dev": "^1.2.0",
    "got": "^11.8.0",
    "ipc-promise-invoke": "^2.1.0",
    "proxy-agent": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.30",
    "uuid": "^8.3.2",
    "vm2": "^3.9.2"
  },
  "build": {
    "files": [
      "./build",
      "./build-electron"
    ],
    "extends": null
  },
  "main": "./build-electron/index.js",
  "nodemonConfig": {
    "ext": "ts,js",
    "exec": "npm run build:core -s && electron ./build-electron"
  },
  "cracoConfig": "config/craco.config.js",
  "scripts": {
    "start:view": "vite --config ./config/vite.config.js",
    "start:view:legacy": "cross-env BROWSER=none craco start",
    "start:core": "nodemon --watch src-electron",
    "start": "concurrently -k \"npm run start:view\" \"npm run start:core -s\"",
    "build:view": "cross-env GENERATE_SOURCEMAP=false craco build",
    "build:core": "rimraf ./build-electron/* && tsc --p ./config/tsconfig-electron.json && cpy \"./assets\" \"../build-electron\" --cwd=\"./src-electron\" --parents",
    "build": "npm run build:view -s && npm run build:core -s && rimraf ./dist/* && electron-builder",
    "test:view": "craco test",
    "postinstall": "electron-builder install-app-deps"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": ".",
  "devDependencies": {
    "@ant-design/icons": "^4.3.0",
    "@craco/craco": "^5.7.0",
    "@rematch/core": "^2.0.0-next.6",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/better-sqlite3": "^5.4.1",
    "@types/d3-scale": "^3.2.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.11",
    "@types/react-router-dom": "^5.1.7",
    "@types/uuid": "^8.3.0",
    "@vitejs/plugin-react-refresh": "^1.3.3",
    "antd": "^4.7.2",
    "concurrently": "^5.3.0",
    "cpy-cli": "^3.1.1",
    "craco-less": "^1.17.0",
    "cross-env": "^7.0.2",
    "d3-scale": "^3.2.3",
    "electron": "^10.1.4",
    "electron-builder": "^22.9.1",
    "node-gyp": "^7.1.2",
    "node-pre-gyp": "^0.17.0",
    "nodemon": "^2.0.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.2.0",
    "react-player": "^2.8.2",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.4",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.3",
    "vite": "^2.2.3",
    "vite-plugin-svgr": "^0.2.0"
  }
}
